  langid = {english},
  keywords = {bacterial transport,Biological physics,Cellular motility,Gels and hydrogels},
  file = {C\:\\Users\\liuzy\\Google Drive\\Zotero\\Bhattacharjee_Datta - 2019 - Bacterial hopping and trapping in porous media.pdf;C\:\\Users\\liuzy\\Zotero\\storage\\HERQZDST\\s41467-019-10115-1.html}
