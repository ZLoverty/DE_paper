  pages = {4655--4660},
  publisher = {{The Royal Society of Chemistry}},
  issn = {1744-6848},
  doi = {10.1039/D0SM00006J},
  abstract = {We study the properties of active noise experienced by a passive particle harmonically trapped in an active bath. The active bath is either explicitly simulated by an ensemble of active Brownian particles or abstractly represented by an active colored noise in theory. Assuming the equivalence of the two descriptions of the active bath, the active noise in the simulation system, which is directly extracted by fitting theoretical predictions to simulation measurements, is shown to depend on the constraint suffered by the passive tracer. This scenario is in significant contrast to the case of thermal noise that is independent of external trap potentials. The constraint dependence of active noise arises from the fact that the persistent force on the passive particle from the active bath can be influenced by the particle relaxation dynamics. Moreover, due to the interplay between the active collisions and particle relaxation dynamics, the effective temperature of the passive tracer quantified as the ratio of fluctuation to dissipation increases as the constraint strengthens, while the average potential and kinetic energies of the passive particle both decrease.},
  langid = {english},
  keywords = {harmonic trap},
  file = {C\:\\Users\\liuzy\\Google Drive\\Zotero\\Ye et al - 2020 - Active noise experienced by a passive particle trapped in an active bath.pdf;C\:\\Users\\liuzy\\Zotero\\storage\\UI6NYP43\\D0SM00006J.html}
}

@article{Yin2022,
  title = {Escaping Speed of Bacteria from Confinement},
  author = {Yin, Yuanfeng and Yu, Hsin-Tzu and Tan, Hong and Cai, Hong and Chen, Hsuan-Yi and Lo, Chien-Jung and Guo, Shuo},
  year = {2022},
