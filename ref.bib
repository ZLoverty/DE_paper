  file = {C\:\\Users\\liuzy\\Google Drive\\Zotero\\Bhattacharjee and Datta - 2019 - Confinement and activity regulate bacterial motion.pdf;C\:\\Users\\liuzy\\Zotero\\storage\\MKJD23X5\\c9sm01735f.html}
}

@article{Bhattacharjee2019a,
